alias apt-update='sudo apt update'
alias apt-upgrade='sudo apt upgrade -y --auto-remove'
alias apt-search='sudo apt search '
alias apt-install='sudo apt install '
alias cfg-vimrc='vim ~/.vim/vimrc'
alias tailf='tail -f'
alias tryd='playonlinux --run tryd'
alias ssh-hml-toyota='ssh -t ubuntu@hml-toyota tmux new-session -s glenon'
alias ssh-prd-toyota='ssh -t ubuntu@prd-toyota tmux new-session -s glenon'
